    "@capacitor/android": "^2.0.0",
    "@capacitor/assets": "^1.0.0-next.5",
    "@capacitor/cli": "^2.0.0",
    "@capacitor/core": "^2.0.0",
    "@capacitor/ios": "^2.0.0",
    "@mdi/font": "^3.6.95",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "core-js": "^3.6.5",
    "highlight.js": "^10.4.1",
    "leaflet": "^1.7.1",
    "roboto-fontface": "*",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.2.0",
    "vue2-leaflet": "^2.7.0",
    "vuetify": "^2.2.11",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@nklayman/vue-cli-plugin-capacitor": "~0.4.0",
